{
  "name": "spaniel",
  "version": "4.0.0",
  "description": "LinkedIn's viewport tracking library",
  "license": "Apache 2.0",
  "main": "exports/spaniel.js",
  "scripts": {
    "test": "tsc && npm run build && testem ci && node test/headless/run",
    "test:headless": "mocha --require babel-core/register test/headless/specs/**/*.js",
    "serve": "npm run build && node test/headless/server/app",
    "watch": "broccoli-timepiece exports",
    "build": "./scripts/build.sh",
    "stats": "node scripts/size-calc",
    "prepublish": "npm run build",
    "docs:build": "./scripts/build-docs.sh",
    "docs:publish": "./scripts/publish-docs.sh"
  },
  "files": [
    "src",
    "exports",
    ".babelrc",
    "Brocfile.js",
    "build.sh",
    "README.md",
    "rollup.config.js",
    "declarations.d.ts",
    "CHANGELOG.md",
    ".travis.yml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/linkedin/spaniel"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": {
    "name": "Asa Kusuma"
  },
  "homepage": "https://github.com/linkedin/spaniel",
  "jsnext:main": "exports/es6/index.js",
  "module": "exports/es6/index.js",
  "typings": "exports/src/index.d.ts",
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "backburner.js": "2.3.0",
    "broccoli": "^1.1.4",
    "broccoli-cli": "1.0.0",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-rollup": "^2.1.1",
    "broccoli-string-replace": "^0.1.2",
    "broccoli-typescript-compiler": "^3.0.1",
    "broccoli-uglify-sourcemap": "^2.2.0",
    "chai": "^4.1.2",
    "chokidar": "^2.0.3",
    "express": "^4.16.3",
    "gzip-size": "^4.1.0",
    "mocha": "^5.2.0",
    "nightmare": "^3.0.1",
    "pretty-bytes": "^5.1.0",
    "rsvp": "^4.8.2",
    "serve-static": "^1.13.2",
    "sinon": "^6.0.0",
    "testem": "^2.8.0",
    "tslint": "^5.10.0",
    "typescript": "2.9.2"
  }
}
